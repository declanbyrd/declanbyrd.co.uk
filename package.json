{
  "name": "declanbyrd.co.uk",
  "version": "1.0.0",
  "description": "My homepage",
  "scripts": {
    "debug": "set DEBUG=Eleventy* & eleventy",
    "prebuild": "npm run sass",
    "build": "npm run sass & npx eleventy",
    "build:eleventy": "npx eleventy",
    "sass": "sass --style=compressed src/scss/styles.scss src/_includes/css/styles.css",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "start": "npm-run-all sass --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/declanbyrd/declanbyrd.co.uk.git"
  },
  "author": {
    "name": "Declan Byrd",
    "email": "declan.byrd@gmail.com",
    "url": "https://www.declanbyrd.co.uk/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/declanbyrd/declanbyrd.co.uk/issues"
  },
  "homepage": "https://github.com/declanbyrd/declanbyrd.co.uk#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "luxon": "^1.24.1",
    "sass": "^1.26.5"
  }
}
