<ol class="content-list flow">
  {% for post in collections.allSocial %}
    <li class="social-card">
      <img class="social-image" src="/img/meta_square.webp" alt="Declan's Twitter avatar" height="60" width="60">
      <div>
        {% include "partials/socialPost.njk" %}
        <span>| <a href="/journal/notes/{{post.date | noteTimestamp}}/">Permalink</a>
        </span>
      </div>
    </li>
  {% endfor %}
</ol>