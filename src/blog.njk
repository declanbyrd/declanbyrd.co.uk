{% extends 'layouts/page.njk' %}


{% block content %}

  <h1>My Blog</h1>

  <p>
    Brain dump of things I do. Things you might see here include code snippets, projects that may or may not be useful and thoughts on life.
  <p>

  <hr>

  {%- for post in collections.posts  -%}
  <section>
    <article>
      <date datetime={{post.date}}>
        {{post.date | dateDisplay}}
      </date>
      <h2>
        <a href="{{post.url}}">
          {{post.data.title | safe}}
        </a>
      </h2>
    </article>
  </section>
 
  {%- endfor -%}

{% endblock %}
